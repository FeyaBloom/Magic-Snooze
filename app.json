{
  "expo": {
    "name": "Snooze Day Magic",
    "slug": "snooze-day-magic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "snoozedaymagic",
    "userInterfaceStyle": "automatic",
    "developmentClient": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.feyabloom.snoozedaymagic",
      "edgeToEdgeEnabled": true,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1b5976c5-36ea-4024-816e-8d0d49e99b9c"
      }
    },
    "owner": "feya_bloom"
  }
}