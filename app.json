{
  "expo": {
    "name": "Magic Snooze",
    "slug": "magic-snooze",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {            
      "image": "./assets/splash-light-full.png",
      "resizeMode": "cover",
      "dark": {
        "image": "./assets/splash-dark-full.png",
        "resizeMode": "cover"
      }
    },
    "scheme": "magic-snooze",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.feyabloom.magicsnooze",
      "edgeToEdgeEnabled": true,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e5a1f91c-ef2b-4751-a1cc-1a443a97e6d4"
      }
    },
    "owner": "feya-bloom"
  }
}
